            color: 0,0,0,1
            size_hint: .25,.25
            pos_hint: {"x":.384, "y":.786}
        MyButton:
            id: yes_button
            on_release: app.root.current = 'createworker'
            text: 'Yes'
            pos_hint: {"x":.306, "y":.57}
        MyButton:
            id: no_button
            on_release: app.root.current = 'createowner'
            text: 'No'
            pos_hint: {"x":.306, "y":.473}

